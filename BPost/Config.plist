<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
	<dict>
		<key>Actions</key>
		<array>
			<dict>
				<key>Image File</key>
				<string>BPost.png</string>
				<key>Regular Expression</key>
				<string>^([0-9]{24})$</string>
				<key>Title</key>
				<string>BPost</string>
				<key>URL</key>
				<string>http://track.bpost.be/etr/light/performSearch.do?oss_language={popclip option language}&amp;searchByItemCode=true&amp;itemCodes={popclip text}</string>
			</dict>
		</array>
		<key>Extension Description</key>
		<string>Track deliveries done with BPost.</string>
		<key>Extension Identifier</key>
		<string>com.tgo.popclip.extension.bpost.itemcode</string>
		<key>Extension Image File</key>
		<string>BPost.png</string>
		<key>Extension Name</key>
		<string>BPost</string>
		<key>Options</key>
		<array>
		 <dict>
			 <key>Option Identifier</key>
			 <string>language</string>
			 <key>Option Label</key>
			 <dict>
				 <key>en</key>
				 <string>Choose your prefered language</string>
			 </dict>
			 <key>Option Type</key>
			 <string>multiple</string>
			 <key>Option Values</key>
			 <array>
				 <string>fr</string>
				 <string>nl</string>
				 <string>de</string>
				 <string>en</string>
			 </array>
		 </dict>
		</array>
	</dict>
</plist>
